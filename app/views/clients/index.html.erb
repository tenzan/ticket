<header>
<h1>Clients</h1>

<% admins_only do %>
<ul class='actions'>
<li><%= link_to 'New Client', new_admin_client_path, class: 'new' %></li>
</ul>
<% end %>

</header>

<div id='clients'>
	<% @clients.each do |client| %>
		<h2><%= link_to client.name, client %></h2>
		<p><%= client.description %></p>
	<% end %>
</div>
